- content_for :head do
	=javascript_include_tag  "jquery-ui-1.8.11.custom.min" ,"autocomplete-rails.js"
#user_form_outer
	#user_form_inner
		%h2 Registrati
		= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f|
			= devise_error_messages!
			%p
				= f.label :email
				%br/
				= f.email_field :email
			%p
				= f.label :user_name
				%br/
				= f.text_field :user_name
			%p
				= f.label :password
				%br/
				= f.password_field :password
			%p
				= f.label :password_confirmation
				%br/
				= f.password_field :password_confirmation
			%p
				= f.label t :height
				%br/
				= f.text_field :height
			%p
				= f.label t :name
				%br/
				= f.text_field :name
			%p
				= f.label t :surname
				%br/
				= f.text_field :surname
			%p
				= f.label t :address
				%br/
				= f.text_field :address

			%p
				= f.label t :city
				%br/
				= f.autocomplete_field :city, autocomplete_city_name_user_index_path
			%p
				= f.label t :country
				%br/
				= f.text_field :country
			%p= f.submit "Registrati"
		= render :partial => "devise/shared/links"
